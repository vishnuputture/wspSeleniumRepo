<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="Domain - Inventory" parallel="tests" thread-count="2" >

    <test name="AverageCost">
        <classes>
            <class name="testcases.Inventory.AverageCost">
                <methods>
                    <include name="Tc_21_SearchItemData"></include>
                    <include name="Tc_22_UpdateItemData"></include>
                    <include name="Tc_23_UpdateItemQuantity"></include>
                    <include name="Tc_24_UpdateItemAVGCost"></include>
                    <include name="Tc_25_ValidateItemCreation"></include>
                    <include name="Tc_26_ValidateItemDeletion"></include>
                    <include name="Tc_27_ValidateItemUpdate"></include>
                    <include name="Tc_28_ValidateReceiveCorrection"></include>
                    <include name="Tc_29_ValidateCostAdjustmentProcess"></include>
                    <include name="Tc_30_ValidateCostAdjustmentLedger"></include>
                    <include name="Tc_31_ValidateMultipleReceiveCorrection"></include>
                    <include name="Tc_32_ValidateMultipleCostAdjustment"></include>
                    <include name="Tc_33_ValidateQuantityFieldReceiveCorrection"></include>
                </methods>
            </class>
        </classes>
    </test>
    <test name="CycleCostAdjust">
        <classes>
            <class name="testcases.Inventory.CycleCostAdjust">
                <methods>
                    <include name="Tc_26_Navigation_to_Cycle_Count_Adjustments"></include>
                    <include name="Tc_27_Verify_assignment_quantity_to_Item"></include>
                    <include name="Tc_29_Negative_Quantity_to_Item"></include>
                    <include name="Tc_30_Navigation_To_Item_Ledger_Screen"></include>
                    <include name="Tc_31_Verify_Item_Ledger_Screen"></include>
                    <include name="Tc_28_assignment_of_quantity_to_multiple_items"></include>
                </methods>
            </class>
        </classes>
    </test>
    <test name="ReceiptCorrection">
        <classes>
            <class name="testcases.Inventory.ReceiptCorrection">
                <methods>
                    <include name="Tc_28_ValidateReceiveCorrection"></include>
                    <include name="Tc_31_ValidateMultipleReceiveCorrection"></include>
                    <include name="Tc_33_ValidateQuantityFieldReceiveCorrection"></include>
                </methods>
            </class>
        </classes>
    </test>
</suite>