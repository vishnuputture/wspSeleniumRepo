                    <include name="TC_371_VerifyDisplayInEBPField"></include>
                    <include name="TC_373_VerifyCreationOfCreditDebitMemoWithoutAccountNo"></include>
                    <include name="TC_374_VerifyCreationOfCreditMemo"></include>
                    <include name="TC_375_VerifyCreationOfDebitMemo"></include>
                    <include name="TC_376_VerifyCMDMTransactionCannotBeModified"></include>

